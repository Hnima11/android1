<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="#000000"
    android:layout_height="match_parent">


    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:layout_gravity="top"
        android:background="#C9F0F5"

        android:padding="16dp">

        <TextView
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="top"
            android:background="000000"
            android:gravity="center"

            android:text="نیما هاشمی"

            android:textColor="#000000"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_gravity="top"
            android:layout_marginTop="30dp"
            android:background="000000"
            android:gravity="center"

            android:text="09333443809"

            android:textColor="#000000"
            android:textSize="25sp" />

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="40dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="50dp"
            android:layout_marginTop="80dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/wallet" />

        <TextView
            android:id="@+id/view3"
            android:layout_width="160dp"
            android:layout_height="50dp"
            android:layout_gravity="top"
            android:layout_marginTop="140dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="فعالسازی کیف پول"

            android:textColor="#000000"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="290dp"
            android:layout_marginTop="93dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/score" />

        <TextView
            android:id="@+id/view4"
            android:layout_width="68dp"
            android:layout_height="50dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="225dp"
            android:layout_marginTop="98dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="امتیاز"

            android:textColor="#000000"
            android:textSize="20sp" />

    </FrameLayout>


    <FrameLayout
        android:id="@+id/frameLayout2"
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:layout_gravity="top"
        android:layout_marginTop="220dp"
        android:background="#C9F0F5"
        android:padding="16dp">

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="75dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="29dp"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/contact" />

        <TextView
            android:id="@+id/view5"
            android:layout_width="120dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="25dp"

            android:layout_marginTop="90dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="تماس با ما"

            android:textColor="#000000"
            android:textSize="30sp" />
        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="270dp"
            android:layout_marginRight="9dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/club" />

        <TextView
            android:id="@+id/view6"
            android:layout_width="130dp"
            android:layout_height="60dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="230dp"

            android:layout_marginTop="90dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="دیجی کلاب"

            android:textColor="#000000"
            android:textSize="30sp" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout3"
        android:layout_width="match_parent"
        android:layout_height="278dp"
        android:layout_gravity="top"

        android:layout_marginTop="430dp"
        android:background="#C9F0F5"
        android:padding="16dp">

        <TextView
            android:id="@+id/view7"
            android:layout_width="196dp"
            android:layout_height="47dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="180dp"

            android:layout_marginTop="3dp"
            android:background="#C9F0F5"
            android:gravity="right"
            android:text="سفارش های من"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold" />

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="75dp"
            android:layout_height="67dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="2dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/cancel" />

        <TextView
            android:id="@+id/view10"
            android:layout_width="100dp"
            android:layout_height="60dp"
            android:layout_gravity="top"

            android:layout_marginHorizontal="75dp"
            android:layout_marginTop="110dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="تحویل شده"

            android:textColor="#000000"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="70dp"
            android:layout_height="67dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="90dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/deliver" />

        <TextView
            android:id="@+id/view8"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:layout_gravity="top"


            android:layout_marginTop="110dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="لغوشده"

            android:textColor="#000000"
            android:textSize="20sp" />
        <TextView
            android:id="@+id/view11"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:layout_gravity="top"

            android:layout_marginHorizontal="175dp"
            android:layout_marginTop="110dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="پردازش"

            android:textColor="#000000"
            android:textSize="20sp" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="75dp"
            android:layout_height="74dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="180dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/pending" />
        <TextView
            android:id="@+id/view12"
            android:layout_width="130dp"
            android:layout_height="60dp"
            android:layout_gravity="top"

            android:layout_marginHorizontal="250dp"
            android:layout_marginTop="110dp"
            android:background="#C9F0F5"
            android:gravity="center"
            android:text="ادرانتظار پرداخت"

            android:textColor="#000000"
            android:textSize="20sp" />
        <ImageButton
            android:id="@+id/imageButton11"
            android:layout_width="75dp"
            android:layout_height="67dp"
            android:layout_gravity="top"
            android:layout_marginHorizontal="280dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/circle_shape"
            android:onClick="onClickMethod"
            android:src="@drawable/waiting_for_payment" />


    </FrameLayout>



</FrameLayout>
